{{ define "mostread_fi" }}
<div id="mostreadweek">
	<div class="mostreadtitle">Luetuimmat</div><div class="mostreadclicks">Klikit</div>
	{{ range .MostReadList }}
		<div class="mostread">
			<div class="date">{{ .PubDate.Local.Format "02.01. 15:04" }}</div><!--
		 --><div class="source">{{ .RssSource }}</div><!--
		 --><div class="category"><a href="/fi/category/{{ toLower .Category.CategoryName }}/0">{{ if eq .Category.CategoryName "Naisetjamuoti" }}Naiset ja muoti{{ else }}{{ .Category.CategoryName }}{{ end }}</a></div><!--
		 --><div class="clicks">{{ .Clicks }}</div>
			<div class="link">
			   <a class="itemClick" class="itemClick" target="_blank" id="{{ .Id.Hex }}" href="{{ .RssLink }}">{{ .RssTitle }}</a>
			</div>
	   </div>
	{{end}}
</div>
{{ end }}
